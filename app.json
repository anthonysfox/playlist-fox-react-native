{
  "expo": {
    "name": "PlaylistFox",
    "slug": "playlistfox",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "playlistfox",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Discover and sync your favorite playlists automatically",
    "privacy": "public",
    "ios": {
      "supportsTablet": true,
      "statusBarStyle": "dark",
      "bundleIdentifier": "com.yourcompany.playlistfox",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "statusBar": {
        "barStyle": "dark-content",
        "backgroundColor": "#F7FAFC"
      },
      "package": "com.yourcompany.playlistfox"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c34b3495-95d7-4184-9bb5-6bd7ddc402ae"
      }
    }
  }
}
